{
    "theme": "light",
    "report_title": "NodeSecure Security Report",
    "report_logo": "https://avatars.githubusercontent.com/u/85318671?s=200&v=4",
    "include_transitive_internal": false,
    "npm_org_prefix": "@nodesecure",
    "npm_packages": [
        "@nodesecure/js-x-ray"
    ],
    "git_url": "https://github.com/NodeSecure",
    "git_repositories": [],
    "charts": [
        {
            "name": "Extensions",
            "display": true,
            "interpolation": "d3.interpolateRainbow"
        },
        {
            "name": "Licenses",
            "display": true,
            "interpolation": "d3.interpolateCool"
        },
        {
            "name": "Warnings",
            "display": true,
            "type": "horizontalBar",
            "interpolation": "d3.interpolateInferno",
            "help": "Documentation of warnings kinds: https://github.com/NodeSecure/js-x-ray"
        },
        {
            "name": "Flags",
            "display": true,
            "type": "horizontalBar",
            "interpolation": "d3.interpolateSinebow",
            "help": "Documentation of flags: https://github.com/NodeSecure/Governance/blob/main/docs/Flags.md"
        }
    ]
}
